<%= m.select :gender, Member::GENDER_TYPES, {include_blank: "Select"}, { class: "select select-primary", data: { toggle: "select"}, disabled: true } %>
<%= m.text_field :ssn, as: :string, title: "SSN", placeholder: "SSN", input_html: { value: number_to_ssn(m.object.ssn), maxlength: 11 }, label: "Social Security", disabled: true %>
<%= m.text_field :dob, as: :string,
  start_year: Date.today.year - 110,
  end_year: Date.today.year,
  placeholder: "mm/dd/yyyy",
  class: "date_picker",
  value: m.object.dob.try(:strftime, "%m/%d/%Y"),
  disabled: true
%>
